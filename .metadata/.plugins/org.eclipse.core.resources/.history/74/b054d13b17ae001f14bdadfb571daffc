package com.Problem_Set_III;

import java.util.Arrays;

public class Scarlar_Product_of_Two_vector {
	public int Product() {
		int[] x= {1,8,4};
		int[] y= {5,4,7};
		Arrays.sort(x);
		Arrays.sort(y);
		int Start=0;
		int End=y.length-1;
		while(Start<End) {
			int temp=0;
			Start=temp;
			Start=End;
			End=temp;
			
			
			Start++;
			End--;
		}
	}
}